{
  "name": "jarvis",
  "productName": "jarvis",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "prebuild": "ts-node src/scripts/download-ffmpeg.ts && ts-node src/scripts/download-whisper-models.ts",
    "build-whisper": "chmod +x ./src/scripts/build-whisper.sh && ./src/scripts/build-whisper.sh",
    "package": "npm run clean-all && npm run prebuild && electron-forge package",
    "make": "npm run clean-all && npm run build-whisper && npm run prebuild && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "postinstall": "npm run clean-all && npm run build-whisper",
    "clean-whisper-buildcache": "rimraf lib/whisper.cpp/build",
    "clean-ffmpeg": "rimraf lib/ffmpeg",
    "clean-whisper-models": "rimraf models",
    "clean-all": "npm run clean-whisper-buildcache && npm run clean-ffmpeg && npm run clean-whisper-models"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/maker-deb": "^7.6.0",
    "@electron-forge/maker-rpm": "^7.6.0",
    "@electron-forge/maker-squirrel": "^7.6.0",
    "@electron-forge/maker-zip": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron-forge/shared-types": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/ffbinaries": "^1.1.5",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cmake-js": "^7.3.0",
    "electron": "33.2.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4",
    "vite": "^5.0.12"
  },
  "keywords": [],
  "author": "abhi",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "electron-squirrel-startup": "^1.0.1",
    "ffbinaries": "^1.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "scheduler": "^0.25.0"
  }
}
